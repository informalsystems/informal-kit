{
  "name": "informal-kit",
  "version": "0.0.22",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "check-types": "npx tsc --noEmit > ts-errors.log",
    "generate-types": ". .env.local && cf-content-types-generator --v10 --spaceId $CONTENTFUL_SPACE_ID --token $CONTENTFUL_MANAGEMENT_ACCESS_TOKEN --out ./lib/contentfulTypes"
  },
  "exports": {
    "./styles/*": "./styles/*",
    "./components/*": "./components/*",
    "./lib/*": "./lib/*"
  },
  "typesVersions": {
    "*": {
      "components/*": [
        "./components/*"
      ],
      "lib/*": [
        "./lib/*"
      ],
      "styles/*": [
        "./styles/*"
      ]
    }
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^17.1.0",
    "@contentful/rich-text-react-renderer": "^16.1.0",
    "@floating-ui/react": "^0.27.15",
    "@gsap/react": "^2.1.2",
    "@headlessui/react": "^2.2.6",
    "contentful": "^11.7.13",
    "floating-ui": "^5.2.8",
    "gsap": "^3.13.0",
    "lodash": "^4.17.21",
    "next": "^15.4.4",
    "react-error-boundary": "^6.0.0",
    "react-markdown": "^10.1.0",
    "react-stickynode": "^5.0.2",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "resend": "^4.7.0",
    "striptags": "^3.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.11.6",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-stickynode": "^4.0.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "cf-content-types-generator": "^2.16.0",
    "contentful-management": "^11.54.3",
    "enquirer": "^2.4.1",
    "eslint": "^9",
    "eslint-config-next": "^15.4.4",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tiny-invariant": "^1.3.3",
    "typescript": "^5"
  }
}
